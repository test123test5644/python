url = "https://script.google.com/macros/s/AKfycbzJJI0RqHFuMTBWnsxlfRllo1gzXzfSOTC1zTui3i_B8npTaC2PPBu9FiwQm5Dz-3q7bA/exec"

response = requests.get(url)

